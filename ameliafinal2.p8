function get_potion(x,y)
		if check_flag(2,x/8,y/8) then
				create_msg("","   bravo!\n   tu as reussi!‚ô•\n\n\n\npress ‚ùé to start again")
		
			 music(-1)
				sfx(1)
				win=true
		end
end

